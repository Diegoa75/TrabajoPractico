@{
    ViewBag.Title = "CrearNuevaSede";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>CrearNuevaSede</h2>

<form action="~/Administrativo/NuevaSede" method="post" class="form-group form-login">
    
    <label>Nombre:</label><input name="Nombre"/>
    <label>Direccion:</label><input name="Direccion"/>
    <label>Precio:</label><input name="PrecioGeneral"/>

    <button type="submit" class="btn btn-info btn-lg btn-block">Crear Sede</button>
</form> 